{
    "configurations": [
        {
            "name": "Test midend debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/run/Midend.elf",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ],
    "version": "2.0.0"
}